<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandCollapseTextView">
        <!-- 收起状态下可以显示的行数 -->
        <attr name="ectv_collapsed_lines" format="integer" />
        <!-- 展开时的提示文字 -->
        <attr name="ectv_prompt_expanded" format="string" />
        <!-- 收起时的提示文字 -->
        <attr name="ectv_prompt_collapsed" format="string" />
    </declare-styleable>
</resources>