<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/wan_android_navigation_main"
    app:startDestination="@id/mMainFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/mMainFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidMainFragment"
        android:label="MainFragment"
        tools:layout="@layout/wan_android_fragment_main">

        <action
            android:id="@+id/mMainToLogin"
            app:destination="@id/mLoginFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
        <action
            android:id="@+id/mMainToSetting"
            app:destination="@id/mSettingFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
        <action
            android:id="@+id/mMainToIntegralRanking"
            app:destination="@id/mIntegralRankingFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
        <action
            android:id="@+id/mMainToSearch"
            app:destination="@id/mSearchFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_fade_enter"
            app:popExitAnim="@anim/fragment_fade_exit" />
        <action
            android:id="@+id/mMainToWeb"
            app:destination="@id/mWebFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
    </fragment>
    <fragment
        android:id="@+id/mLoginFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidLoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/wan_android_fragment_login">
        <action
            android:id="@+id/mLoginToRegister"
            app:destination="@id/mRegisterFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
    </fragment>
    <fragment
        android:id="@+id/mRegisterFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidRegisterFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/wan_android_fragment_register">
        <action
            android:id="@+id/mRegisterToMain"
            app:destination="@id/mMainFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
    </fragment>
    <fragment
        android:id="@+id/mIntegralRankingFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidIntegralRankingFragment"
        android:label="IntegralRankingFragment"
        tools:layout="@layout/wan_android_fragment_integral_ranking">
        <action
            android:id="@+id/mIntegralRankingToWeb"
            app:destination="@id/mWebFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
        <action
            android:id="@+id/mIntegralRankingToIntegralRecord"
            app:destination="@id/mIntegralRecordFragment"
            app:enterAnim="@anim/pull_in_right"
            app:exitAnim="@anim/push_out_left"
            app:popEnterAnim="@anim/pull_in_left"
            app:popExitAnim="@anim/push_out_right" />
    </fragment>
    <fragment
        android:id="@+id/mIntegralRecordFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidIntegralRecordFragment"
        android:label="IntegralRecordFragment"
        tools:layout="@layout/wan_android_fragment_integral_record" />
    <fragment
        android:id="@+id/mSettingFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidSettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/wan_android_fragment_setting" />
    <fragment
        android:id="@+id/mSearchFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidSearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/wan_android_fragment_search" />
    <fragment
        android:id="@+id/mWebFragment"
        android:name="com.wkz.wanandroid.mvvm.view.fragment.WanAndroidWebFragment"
        android:label="WebFragment"
        tools:layout="@layout/framework_layout_base_web" />
</navigation>