<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/mVpDiscovery"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.fphoenixcorneae.titlebar.CommonTitleBar
        android:id="@+id/mTbTitleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:centerCustomView="@layout/open_eyes_title_bar_custom_center"
        app:centerType="customView"
        app:layout_constraintTop_toTopOf="parent"
        app:showBottomLine="false"
        app:titleBarHeight="?attr/actionBarSize" />
</androidx.constraintlayout.widget.ConstraintLayout>